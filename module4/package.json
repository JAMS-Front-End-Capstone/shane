{
  "name": "app",
  "version": "1.0.0",
  "description": "service component for app",
  "main": "index.js",
  "scripts": {
    "backend": "nodemon backend/server.js",
    "frontend": "live-server --port=3005 --watch=frontend/dist --wait=1000 frontend/dist",
    "build-dev": "webpack --config webpack.config.js --watch -d source-map",
    "build": "webpack --config webpack.config.js --watch",
    "start": "node backend/index.js",
    "seed": "node backend/modules/seed.js",
    "test": "jest"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules/*",
      "frontend/*"
    ],
    "watch": [
      "backend/*"
    ],
    "delay": "1000"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "node": "^15.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "webpack": "^5.13.0",
    "webpack-cli": "^4.3.1",
    "live-server": "^1.2.1",
    "babel-loader": "^8.2.2",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-jest": "^26.6.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "css-loader": "^5.0.1",
    "express": "^4.17.1",
    "faker": "^5.1.0",
    "jest": "^26.6.3",
    "mongoose": "^5.11.11",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "react-test-renderer": "^17.0.1",
    "style-loader": "^2.0.0"
  }
}
